<p id="notice"><%= notice %></p>

<h1>Form Templates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @form_templates.each do |form_template| %>
      <tr>
        <td><%= form_template.name %></td>
        <td><%= link_to 'Show', form_template %></td>
        <td><%= link_to 'Edit', edit_form_template_path(form_template) %></td>
        <td><%= link_to 'Destroy', form_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Form Template', new_form_template_path %>
